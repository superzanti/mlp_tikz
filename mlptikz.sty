%----------------------------------------------------------------------------------------
%    Base Code
%----------------------------------------------------------------------------------------
\newcommand{\getmlptikz}[9]{
	\ifstrequal{#1}{#2}{%
    	\edef\colorval{{\commandkey{color}}}
	\edef\textval{{\commandkey{text}}}
        %I need to add error checking for invalid inputs
        \expandafter\ifstrequal\colorval{mono}{%
		#3
        }{%
		#7
	}
        \expandafter\ifstrequal\colorval{custom}{%
        	#4
        }{}
        \expandafter\ifstrequal\colorval{black}{%
        	#5
        }{}
        \expandafter\ifstrequal\colorval{invert}{%
        	#6
        }{}
		\resizebox{\commandkey{size}}{!}{%
			\begin{tikzpicture}[y=\commandkey{yscale}, x=\commandkey{xscale}, yscale=-\commandkey{flipy}, xscale=\commandkey{flipx}, inner sep=0pt, outer sep=0pt]
				\begin{scope}[cm=#9,rotate=\commandkey{rotate},opacity=\commandkey{opacity}]
					#8
					\expandafter\ifstrequal\textval{on}{%
					\node[\commandkey{textshape},draw=\commandkey{textshapebordercolor}, text opacity=1, opacity=\commandkey{textshapeopacity}, yscale=\commandkey{textscale},xscale=\commandkey{textscale}, fill=\commandkey{textshapecolor}, line width=\commandkey{textshapeborder}, rotate=\commandkey{textshaperotate}, inner sep=0pt,minimum size=14pt, align=center] (1) at(0,0) {\,{\color{\commandkey{textcolor}}{\commandkey{textvalue}}}\,};
					}{}
				\end{scope}
			\end{tikzpicture}
		}
	}{\typeout{Error in mlptikz: #2 is not a valid input}}
}

%----------------------------------------------------------------------------------------
%    Main Function Call
%----------------------------------------------------------------------------------------

\newkeycommand\mlptikz[size=1in,%		% size can have any numbered value and scales it to an x by x box
	color=full,%				% color can be full (for the default colors), mono (for monochrome), or custom (to define your own)
	ccolor1a=128,%				% the following values are for each of your custom colors, you'll need the table to find out what goes where
	ccolor1b=128,%
	ccolor1c=128,%
	ccolor2a=128,%
	ccolor2b=128,%
	ccolor2c=128,%
	ccolor3a=128,%
	ccolor3b=128,%
	ccolor3c=128,%
	ccolor4a=128,%
	ccolor4b=128,%
	ccolor4c=128,%
	ccolor5a=128,%
	ccolor5b=128,%
	ccolor5c=128,%
	opacity=1,%				% opacity is a value from 0 to 1 which changes transparency
	text=off,%				% want to put a page number or text inside these icons!? Set this to on or off
	textvalue=test,%			% what text should be displayed?
	textposx=0,%				% the position of the text horizontally
	textposy=0,%				% the position of the text vertically
	textcolor=black,%			% change the color of the text
	textscale=10,%				% make the text in here smaller and bigger
	textshape=ellipse,%			% you probably want some kind of background for your text, ellipse or rectangle?
	textshapecolor=white,%			% change the background color
	textshapeborder=0.5mm,%			% change the thickness of the border
	textshapebordercolor=black,%		% change the color of the border
	textshapeopacity=1,%			% what is the opacity of your background? These are values from 0 to 1
	textshaperotate=0,%			% rotate the text from -360 to 360 (45 will rotate it 45 degrees clockwise)
	rotate=0,%				% need to rotate one of these icons? do it in degrees from -360 to 360
	flipx=1,%				% flip the image horizontally! Use -1 to flip and 1 to keep the default
	flipy=1,%				% flip the image vertically! Use -1 to flip and 1 to keep the default
	xscale=0.8,%				% need to scale the image? change this number to any value!
	yscale=0.8][1]{				% change this number to any value to strech and shrink vertically

